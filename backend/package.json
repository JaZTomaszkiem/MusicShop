{
  "name": "docker_web_app",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && tsc -p .",
    "clean": "rm -rf dist/",
    "dev": "nodemon -L -e js,ts,json --exec \"npm run start\"",
    "start": "node -r dotenv/config --enable-source-maps dist/index.js dotenv_config_path=env-local.env",
    "package": "cd dist && tar --exclude='*.map' -zcf ../artifact.tar.gz . && cd ..",
    "prestart": "npm run -s build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.46.0",
    "@aws-sdk/lib-storage": "^3.46.0",
    "@babel/cli": "^7.16.0",
    "@google-cloud/storage": "^5.18.1",
    "aws-sdk": "^2.1041.0",
    "bcrypt": "^5.0.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "content-hash": "^2.5.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "ipfs-http-client": "^54.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.2",
    "nodemon": "^2.0.15",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "web3": "^1.6.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.19",
    "@types/uuid": "^8.3.4",
    "dotenv": "^10.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
}
